<?xml version="1.0" encoding="UTF-8"?>
<exam>
	<question id="0" type="single">
		<qtext>Which company originally developed Java?</qtext>
		<choices>
			<choice id="1">Microsoft</choice>
			<choice id="2">Sun Microsystems</choice>
			<choice id="3">Oracle</choice>
			<choice id="4">IBM</choice>
		</choices>
		<answer>2</answer>
	</question>

	<question id="1" type="single">
		<qtext>Which keyword is used to inherit a class in Java?</qtext>
		<choices>
			<choice id="1">this</choice>
			<choice id="2">implements</choice>
			<choice id="3">extends</choice>
			<choice id="4">super</choice>
		</choices>
		<answer>3</answer>
	</question>

	<question id="2" type="multiple">
		<qtext>Which of the following are features of Java?</qtext>
		<choices>
			<choice id="1">Platform Independent</choice>
			<choice id="2">Object-Oriented</choice>
			<choice id="3">Pointer Arithmetic</choice>
			<choice id="4">Robust</choice>
		</choices>
		<answer>1,2,4</answer>
	</question>

	<question id="3" type="single">
		<qtext>Which collection class allows you to grow or shrink its size and provides indexed access to its elements?</qtext>
		<choices>
			<choice id="1">HashMap</choice>
			<choice id="2">ArrayList</choice>
			<choice id="3">LinkedList</choice>
			<choice id="4">TreeMap</choice>
		</choices>
		<answer>2</answer>
	</question>

	<question id="4" type="multiple">
		<qtext>Which of the following are checked exceptions in Java?</qtext>
		<choices>
			<choice id="1">IOException</choice>
			<choice id="2">NullPointerException</choice>
			<choice id="3">SQLException</choice>
			<choice id="4">ArrayIndexOutOfBoundsException</choice>
		</choices>
		<answer>1,3</answer>
	</question>

	<question id="5" type="single">
		<qtext>Which method is the entry point of a Java program?</qtext>
		<choices>
			<choice id="1">start()</choice>
			<choice id="2">main()</choice>
			<choice id="3">run()</choice>
			<choice id="4">init()</choice>
		</choices>
		<answer>2</answer>
	</question>
	<question id="6" type="single">
		<qtext>What is the size of an int variable in Java?</qtext>
		<choices>
			<choice id="1">8 bits</choice>
			<choice id="2">16 bits</choice>
			<choice id="3">32 bits</choice>
			<choice id="4">64 bits</choice>
		</choices>
		<answer>3</answer>
	</question>

	<question id="7" type="single">
		<qtext>Which of the following is NOT a Java access modifier?</qtext>
		<choices>
			<choice id="1">public</choice>
			<choice id="2">private</choice>
			<choice id="3">protected</choice>
			<choice id="4">internal</choice>
		</choices>
		<answer>4</answer>
	</question>

	<question id="8" type="multiple">
		<qtext>Which of the following are valid Java data types?</qtext>
		<choices>
			<choice id="1">int</choice>
			<choice id="2">float</choice>
			<choice id="3">decimal</choice>
			<choice id="4">double</choice>
		</choices>
		<answer>1,2,4</answer>
	</question>

	<question id="9" type="single">
		<qtext>What does the 'final' keyword mean when applied to a variable?</qtext>
		<choices>
			<choice id="1">The variable can be modified</choice>
			<choice id="2">The variable cannot be modified</choice>
			<choice id="3">The variable is static</choice>
			<choice id="4">The variable is volatile</choice>
		</choices>
		<answer>2</answer>
	</question>

	<question id="10" type="single">
		<qtext>Which method is used to start a thread execution in Java?</qtext>
		<choices>
			<choice id="1">start()</choice>
			<choice id="2">run()</choice>
			<choice id="3">execute()</choice>
			<choice id="4">init()</choice>
		</choices>
		<answer>1</answer>
	</question>

	<question id="11" type="single">
		<qtext>Which package contains the Random class?</qtext>
		<choices>
			<choice id="1">java.util</choice>
			<choice id="2">java.lang</choice>
			<choice id="3">java.io</choice>
			<choice id="4">java.net</choice>
		</choices>
		<answer>1</answer>
	</question>

