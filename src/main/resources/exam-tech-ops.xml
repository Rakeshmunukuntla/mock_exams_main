        <choices>
            <choice id="1">Development</choice>
            <choice id="2">Quality Assurance</choice>
            <choice id="3">Production</choice>
            <choice id="4">Training</choice>
        </choices>
        <answer>1,2,3,4</answer>
    </question>

    <question id="3" type="single">
