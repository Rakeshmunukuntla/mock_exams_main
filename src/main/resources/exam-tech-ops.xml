<exam>

    <question id="0" type="single">
        <qtext>What is SAP NetWeaver?</qtext>
        <choices>
            <choice id="1">A hardware component</choice>
            <choice id="2">An application server and technology platform</choice>
            <choice id="3">A database</choice>
            <choice id="4">A user interface tool</choice>
        </choices>
        <answer>2</answer>
    </question>

    <question id="1" type="single">
        <qtext>Which tool is used to monitor SAP system performance?</qtext>
        <choices>
            <choice id="1">SAP Solution Manager</choice>
            <choice id="2">SAP GUI</choice>
            <choice id="3">SAP Business One</choice>
            <choice id="4">SAP Fiori</choice>
        </choices>
        <answer>1</answer>
    </question>

    <question id="2" type="multiple">
